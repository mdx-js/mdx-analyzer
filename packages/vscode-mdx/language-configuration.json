{
  "comments": {
    "blockComment": ["{/*", "*/}"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": ["string"]
    },
    {
      "open": "<",
      "close": ">",
      "notIn": ["string"]
    },
    {
      "open": "/**",
      "close": " */",
      "notIn": ["string"]
    },
    {
      "open": "{/*",
      "close": "*/}",
      "notIn": ["string", "comment"]
    }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["<", ">"],
    ["_", "_"],
    ["*", "*"]
  ],
  "folding": {
    "offSide": true,
    "markers": {
      "start": "^\\s*{/*\\s*#?region\\b.**/}",
      "end": "^\\s*{/*\\s*#?endregion\\b.**/}"
    }
  }
}
