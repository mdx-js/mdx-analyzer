{
  "comments": {
    "blockComment": ["{/*", "*/}"]
  },
  // symbols used as brackets
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  // symbols that are auto closed when typing
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["`", "`"],
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": ["string"]
    },
    {
      "open": "<",
      "close": ">",
      "notIn": ["string"]
    },
    {
      "open": "/**",
      "close": " */",
      "notIn": ["string"]
    },
    {
      "open": "{/*",
      "close": "*/}",
      "notIn": ["string", "comment"]
    }
  ],
  // symbols that that can be used to surround a selection
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["<", ">"],
    ["`", "`"],
    ["_", "_"],
    ["*", "*"]
  ],
  "folding": {
    "offSide": true,
    "markers": {
      "start": "^\\s*{/*\\s*#?region\\b.**/}",
      "end": "^\\s*{/*\\s*#?endregion\\b.**/}"
    }
  }
}
